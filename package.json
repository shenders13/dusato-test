{
  "name": "dusato",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "http": "0.0.0",
    "isomorphic-fetch": "^2.2.1",
    "mongoose": "^4.9.3",
    "path": "^0.12.7",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.5.2",
    "react-alert": "^1.0.14",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.4.2",
    "socket.io": "1.1.*",
    "jade": "1.6.*",
    "express": "4.8.*",
    "serve-favicon": "2",
    "morgan": "1.3.*",
    "method-override":"*",
    "body-parser": "1.8",
    "errorhandler": "*",
    "express-session": "*",
    "multer": "0.1.*"
  },
  "devDependencies": {
    "react-scripts": "0.9.5"
  },
  "scripts": {
    "start": "if [ \"$NODE_ENV\" = \"production\" ]; then npm run serve-prod; else npm run serve-dev; fi",
    "postinstall": "if [ \"$NODE_ENV\" = \"production\" ]; then npm run build; fi",
    "build": "NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --colors -p",
    "serve-dev": "NODE_ENV=development node internals/webpack/server.dev.js",
    "serve-prod": "NODE_ENV=production node internals/webpack/server.prod.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
